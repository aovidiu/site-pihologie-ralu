{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Angular: Launch Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/src",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "localhost:4200/site-psihologie-ralu/*": "${workspaceFolder}/*"
      }
    },
    {
      "name": "Angular: Attach to Chrome",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "webRoot": "${workspaceFolder}/src",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "localhost:4200/site-psihologie-ralu/*": "${workspaceFolder}/*"
      }
    }
  ]
}
